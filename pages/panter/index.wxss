page {
  background: #fff;
}

.body {
  padding: 0 70rpx;
  box-sizing: border-box;
}

.bg {
  background: #f1f1f1;
  height: 20rpx;
}

.commodity_attr_box {
  width: 100%;
}

.header {
  color: #333;
  font-size: 36rpx;
  height: 100rpx;
  line-height: 100rpx;
}

.inputView {
  height: 100rpx;
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  line-height: 100rpx;
  border-bottom: 1px #dbdbdb solid;
  font-size: 30rpx;
}

.tubiao {
  float: left;
}

.tubiao text {
  color: #fe4b33;
  font-size: 30rpx;
}

.inputText {
  width: 78%;
  height: 100rpx;
  color: #c2c2c2;
  font-size: 30rpx;
  float: left;
}

.inputText_pwd {
  width: 100%;
  height: 100rpx;
  color: #c2c2c2;
  font-size: 30rpx;
  float: left;
}

.loginBtn {
  width: 100%;
  height: 94rpx;
  line-height: 94rpx;
  margin-top: 117rpx;
  border-radius: 10rpx;
  color: #fff;
  font-size: 36rpx;
  background: #fe4b33;
}

.nameImage {
  width: 12rpx;
  height: 22rpx;
  background-size: 100%;
  vertical-align: middle;
  margin: 0 20rpx;
}

.provtext {
  width: 120px;
  height: 100rpx;
  float: left;
  line-height: 100rpx;
  box-sizing: border-box;
}

.proving {
  width: 160rpx;
  height: 50rpx;
  background: #fe4b33;
  color: #fff;
  text-align: center;
  float: right;
  border-radius: 5px;
  line-height: 50rpx;
  font-size: 24rpx;
  margin-top: 25rpx;
}

.agree {
  color: #333;
  font-size: 26rpx;
  margin-top: 26rpx;
}

.agree text {
  color: #fe4b33;
}

.agree image {
  width: 30rpx;
  height: 30rpx;
  background-size: 100%;
  vertical-align: middle;
  margin-top: -4rpx;
  margin-right: 17rpx;
}

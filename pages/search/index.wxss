page {
  background: #f1f1f1;
}

.header {
  width: 100%;
  height: 88rpx;
  box-sizing: border-box;
  /* position: fixed;
  top: 3rpx; */
  z-index: 999;
  background: #fff;
  box-shadow: 0px 3px 5px #f4f4f4;
  margin-top: 3rpx;
}

.header_cate {
  width: 50%;
  display: inline-block;
  font-size: 36rpx;
  text-align: center;
  line-height: 88rpx;
  height: 88rpx;
}

.header_cate:nth-child(1) {
  border-right: 1px #ccc solid;
  box-sizing: border-box;
}

.header_cate image {
  width: 15px;
  height: 15px;
  background-size: cover;
  vertical-align: middle;
  text-align: center;
  margin-top: -2px;
}

.show {
  display: block;
}

.hidden {
  display: none;
}

.titail {
  padding: 10rpx;
}

.select {
  color: #666;
}

.normal {
  color: #fe4b33;
  border-bottom: 2px red solid;
}

.search {
  width: 100%;
  height: 96rpx;
  padding-top: 20rpx;
  background: #f5f5f5;
  /* height: 186rpx; *//* padding-top: 111rpx; */
  box-sizing: border-box;
}

.inputtext {
  width: 82%;
  background: #ebebeb;
  height: 57rpx;
  border-radius: 15rpx;
  font-size: 26rpx;
  padding-left: 17rpx;
  margin-left: 14rpx;
  line-height: 57rpx;
  float: left;
}

.inputtext image {
  width: 30rpx;
  height: 30rpx;
  background-size: 100%;
  vertical-align: middle;
  margin-right: 13rpx;
  float: left;
  margin-top: 16rpx;
}

.text_style {
  width: 90%;
  height: 57rpx;
  line-height: 57rpx;
  float: left;
}

.search_button {
  font-size: 32rpx;
  color: #00c203;
  float: right;
  margin-right: 20rpx;
}

.content {
  width: 100%;
}

.shoplist {
  width: 100%;
  height: 184rpx;
  padding-top: 32rpx;
  box-sizing: border-box;
  border-bottom: 1px #f4f4f4 solid;
  background: #fff;
}

.pictureUrl_style {
  float: left;
  margin-right: 20rpx;
  margin-left: 30rpx;
  overflow: hidden;
  width: 120rpx;
  height: 120rpx;
  position: relative;
}
.redius{
  border-radius: 50%;
}

.picture_img {
  width: 120rpx;
  height: 120rpx;
  background: #dfdfdf;
}

.listy {
  width: 65%;
  float: left;
}

.Name_style {
  float: left;
  font-size: 34rpx;
  width: 100%;
  height: 50rpx;
  overflow: hidden;
  color: #000;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 5rpx;
}

.Namecolor {
  color: #a9a9a9;
  font-size: 26rpx;
  width: 100%;
  height: 35rpx;
  overflow: hidden;
  line-height: 35rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.right_styleimg {
  width: 32rpx;
  height: 32rpx;
  float: right;
  margin-right: 20rpx;
  margin-top: 38rpx;
}

.history {
  width: 100%;
  padding: 40rpx 24rpx 0 20rpx;
  box-sizing: border-box;
}

.history_search {
  width: 100%;
  font-size: 26rpx;
  height: 40rpx;
  color: #666;
  float: left;
  margin-bottom: 40rpx;
}

.history_search image {
  width: 30rpx;
  height: 40rpx;
  background-size: 100%;
  float: right;
}

.history_name {
  background: #cdcdcd;
  padding: 5rpx 35rpx;
  font-size: 26rpx;
  color: #fff;
  float: left;
  border-radius: 30rpx;
  margin-right: 20rpx;
  margin-bottom: 41rpx;
}
.position_ab {
  position: absolute;
  bottom: 0;
  right: 0;
  width: 36rpx;
  height: 26rpx;
}

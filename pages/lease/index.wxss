page {
  background: #f1f1f1;
}

.body {
  width: 100%;
}

.content {
  width: 100%;
  padding-top: 20rpx;
}

.shoplist {
  width: 100%;
  height: 184rpx;
  padding-top: 32rpx;
  box-sizing: border-box;
  border-bottom: 1px #f4f4f4 solid;
  background: #fff;
}

.pictureUrl_style {
  float: left;
  margin-right: 20rpx;
  margin-left: 30rpx;
  overflow: hidden;
  width: 120rpx;
  height: 120rpx;
}

.picture_img {
  width: 120rpx;
  height: 120rpx;
  background: #dfdfdf;
}

.listy {
  width: 65%;
  float: left;
}

.Name_style {
  float: left;
  font-size: 34rpx;
  width: 100%;
  height: 50rpx;
  overflow: hidden;
  color: #000;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 5rpx;
}

.Namecolor {
  color: #a9a9a9;
  font-size: 26rpx;
  width: 100%;
  height: 35rpx;
  overflow: hidden;
  line-height: 35rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.right_styleimg {
  width: 32rpx;
  height: 32rpx;
  float: right;
  margin-right: 20rpx;
  margin-top: 38rpx;
}

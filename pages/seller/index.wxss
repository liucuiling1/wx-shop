page {
  background: #f1f1f1;
}

.body {
  width: 100%;
}

.userinfo {
  width: 100%;
  height: 148rpx;
  background: linear-gradient(#fe4b33, #fe8333);
  padding-top: 24rpx;
  box-sizing: border-box;
  float: left;
}

.userinfo-avatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-left: 20rpx;
  float: left;
  margin-right: 20rpx;
}

.userinfo-nickname {
  color: #fff;
}

.userinfo_header {
  width: 50%;
  height: 140rpx;
  float: left;
  font-size: 32rpx;
  color: #fff;
}

.number {
  font-size: 26rpx;
  margin-top: 10rpx;
}

.right {
  float: right;
  width: 24rpx;
  height: 40rpx;
  background-size: 100%;
  margin-right: 20rpx;
  margin-top: 30rpx;
}

.guanzhu {
  float: right;
  width: 90rpx;
  height: 50rpx;
  border: 1px #e7e7e8 solid;
  color: #fff;
  font-size: 26rpx;
  text-align: center;
  line-height: 50rpx;
  margin-top: 25rpx;
  margin-right: 40rpx;
  border-radius: 10rpx;
}

.header {
  width: 100%;
  height: 88rpx;
  overflow-x: auto;
  overflow-y: hidden;
  box-sizing: border-box;
  white-space: nowrap;
  z-index: 99;
  background: #fff;
  line-height: 88rpx;
  padding-left: 12rpx;
}

.header_cate {
  padding: 0 42rpx;
  height: 80rpx;
  z-index: 100;
  display: inline-block;
  font-size: 30rpx;
}

.titail {
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  display: inline-block;
}

.select {
  color: #666;
}

.normal {
  color: #fe4b33;
  border-bottom: 2px #fe4b33 solid;
}

.content {
  width: 100%;
  margin-top: 10rpx;
}

.shoplist {
  width: 100%;
  height: 190rpx;
  padding-top: 32rpx;
  box-sizing: border-box;
  border-bottom: 1px #f4f4f4 solid;
  background: #fff;
}

.pictureUrl_style {
  float: left;
  margin-right: 20rpx;
  margin-left: 30rpx;
  overflow: hidden;
  width: 120rpx;
  height: 120rpx;
}

.picture_img {
  width: 120rpx;
  height: 120rpx;
  background: #dfdfdf;
}

.listy {
  width: 65%;
  float: left;
}

.Name_style {
  float: left;
  font-size: 34rpx;
  width: 100%;
  height: 50rpx;
  overflow: hidden;
  color: #000;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 5rpx;
}

.Namecolor {
  color: #a9a9a9;
  font-size: 26rpx;
  width: 100%;
  height: 35rpx;
  overflow: hidden;
  line-height: 35rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.right_styleimg {
  width: 32rpx;
  height: 32rpx;
  float: right;
  margin-right: 20rpx;
  margin-top: 38rpx;
}

.fixed {
  position: fixed;
  bottom: 204rpx;
  left: 30rpx;
  width: 90rpx;
  height: 90rpx;
}

.fixed image {
  width: 90rpx;
  height: 90rpx;
  background-size: 100%;
}

page {
  background: #f1f1f1;
}

.container {
  width: 100%;
  background: #f1f1f1;
}

.userinfo {
  width: 100%;
  height: 160rpx;
  background: linear-gradient(#fe4b33, #fe8333);
  padding-top: 16rpx;
  box-sizing: border-box;
}

.userinfo_view {
  float: left;
  width: 128rpx;
  height: 128rpx;
  position: relative;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}

.position_ab {
  position: absolute;
  bottom: 5rpx;
  right: 10rpx;
  width: 36rpx;
  height: 26rpx;
}

.userinfo-nickname {
  color: #fff;
}

.header {
  width: 50%;
  height: 140rpx;
  float: left;
  font-size: 40rpx;
  color: #fff;
}

.stylecolor {
  font-size: 32rpx;
}

.right {
  float: right;
  width: 65rpx;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
}

.right image {
  width: 22rpx;
  height: 40rpx;
  background-size: 100%;
  vertical-align: middle;
}

.userimg {
  margin-bottom: 2px;
  margin-right: 5px;
  vertical-align: middle;
  width: 50rpx;
  height: 50rpx;
}

.my-item {
  width: 100%;
  border-bottom: 1px solid #eee;
  height: 56px;
  line-height: 56px;
  font-size: 14px;
}

.orderInfo {
  background: #fff;
  margin-bottom: 10px;
}

.allOrder text {
  display: inline-block;
  color: #1b1b1b;
  font-size: 14px;
}

.allOrder view {
  display: inline-block;
  position: absolute;
  right: 20px;
  color: #fc6a8c;
  font-size: 14px;
}

.allOrder view image {
  display: inline-block;
  width: 24px;
  height: 24px;
  margin-top: 15px;
}

.news-item {
  background: #fff;
  padding-left: 20rpx;
  box-sizing: border-box;
  border-bottom: 1px #fafafa solid;
}

.news-item:nth-child(2) {
  margin-bottom: 50rpx;
}

.news-item:nth-child(6) {
  margin-bottom: 50rpx;
}

.news-item-title {
  color: #666;
  font-size: 32rpx;
  height: 100rpx;
  line-height: 100rpx;
}

.right_image {
  width: 32rpx;
  height: 32rpx;
  margin-left: 15rpx;
  margin-right: 20rpx;
  background-size: 100%;
  float: right;
  vertical-align: middle;
  margin-top: 4px;
}

.img_style {
  width: 16rpx;
  height: 28rpx;
  float: right;
  margin: 30rpx 20rpx 0 12rpx;
  background-size: 100%;
}

.number {
  font-size: 28rpx;
  color: #fff;
  margin-right: 20rpx;
  float: right;
  background: #e9553e;
  width: 70rpx;
  height: 40rpx;
  margin-top: 25rpx;
  text-align: center;
  line-height: 40rpx;
  border-radius: 60rpx;
}

/* 弹出层 */

.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.6;
  z-index: 88;
  color: #fff;
  overflow: hidden;
}

/* 弹出页面 */

.commodity_attr_box {
  position: fixed;
  bottom: 23%;
  left: 2.5%;
  z-index: 99;
  height: 560rpx;
  width: 95%;
  background: #fff;
  border-radius: 15px;
  overflow: hidden;
  padding: 50rpx 42rpx;
  box-sizing: border-box;
}

.inputView {
  height: 100rpx;
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  line-height: 100rpx;
  border-bottom: 1px #dbdbdb solid;
}

.tubiao {
  float: left;
}

.tubiao text {
  color: #fe4b33;
  font-size: 30rpx;
}

.inputText {
  width: 78%;
  height: 100rpx;
  color: #c2c2c2;
  font-size: 34rpx;
  float: left;
}

.inputText_pwd {
  width: 100%;
  height: 100rpx;
  color: #c2c2c2;
  font-size: 34rpx;
  float: left;
}

/*按钮*/

.loginBtnView {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}

.loginBtn {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  margin-top: 60rpx;
  border-radius: 10rpx;
  color: #fff;
  font-size: 32rpx;
  background: #fe4b33;
}

.Registerbtn {
  float: right;
  color: #fe4b33;
  font-size: 26rpx;
}

.Registerbtn_left {
  float: left;
  color: #c2c2c2;
  font-size: 26rpx;
}

.nameImage {
  width: 12rpx;
  height: 22rpx;
  background-size: 100%;
  vertical-align: middle;
  margin: 0 20rpx;
}

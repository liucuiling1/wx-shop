page {
  background: #fff;
}

.body {
  width: 100%;
}

.heater {
  width: 100%;
  height: 130rpx;
  color: #e94c36;
  font-size: 40rpx;
  text-align: center;
  font-weight: bold;
  padding-top: 26rpx;
  box-sizing: border-box;
  background: #efefef;
}

.margintop {
  margin-top: 20rpx;
}

.margintop:nth-child(1) {
  margin-top: 0rpx;
}

.ba_img {
  width: 100%;
  height: 78rpx;
  background-size: 100%;
  box-sizing: border-box;
  line-height: 78rpx;
}


.heater image {
  width: 28rpx;
  height: 28rpx;
  background-size: 100%;
  margin-right: 20rpx;
  margin-left: 20rpx;
  vertical-align: middle;
  margin-top: -5rpx;
}

.center {
  width: 100%;
  box-sizing: border-box;
  background: #fff;
}

.imgstyle {
  width: 24.8%;
  height: 246rpx;
  float: left;
  background: #fff;
  margin-right: 2rpx;
  box-sizing: border-box;
}

.imgstyle:nth-child(4) {
  margin-right: 0;
}

.imgstyle:nth-child(8) {
  margin-right: 0;
}

.center_hrader {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  box-sizing: border-box;
  color: #666;
  font-size: 30rpx;
  padding-left: 20rpx;
}

.imgstyle image {
  width: 100%;
  height: 186rpx;
  background-size: 100%;
  box-sizing: border-box;
  background: #f4f4f4;
}
